extends layout

block content
  h1= title
  p Welcome to #{title}. My name is Sidhartha Gundarapu
  - var image_names = ["To Kill a Mockingbird.jpg","Pride and Prejudice.jpg","Reprieve.jpg","Eyre.jpg","DonQuixote.jpg"]
  - for(let i=0; i<3; i++){
    - var j=Math.floor(Math.random()*5);
    - var img_j=image_names[j];
    image(height="300" width="300" src=`\\images\\${img_j}` alt=`${img_j}` )
  - }